  "components": {
    "schemas": {
      "ClienteInputModel": {
        "required": [
          "celular",
          "cpf",
          "dataNascimento",
          "email",
          "endereco",
          "nome",
          "rendaMensal",
          "senha"
        ],
        "type": "object",
        "properties": {
          "nome": {
            "minLength": 1,
            "type": "string"
          },
          "cpf": {
            "maxLength": 11,
            "minLength": 11,
            "type": "string"
          },
          "dataNascimento": {
            "type": "string",
            "format": "date-time"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "senha": {
            "minLength": 1,
            "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[\\W_]).{8,}$",
            "type": "string"
          },
          "celular": {
            "minLength": 1,
            "type": "string"
          },
          "rendaMensal": {
            "type": "number",
            "format": "double"
          },
          "endereco": {
            "$ref": "#/components/schemas/EnderecoInputModel"
          }
        },
        "additionalProperties": false
      },
      "ContaInputModel": {
        "required": [
          "clienteId",
          "numero",
          "saldo",
          "tipo"
        ],
        "type": "object",
        "properties": {
          "numero": {
            "maximum": 99999,
            "minimum": 1000,
            "type": "integer",
            "format": "int32"
          },
          "saldo": {
            "maximum": 10000,
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          },
          "tipo": {
            "enum": [
              "Corrente",
              "Poupanca"
            ],
            "type": "string"
          },
          "clienteId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "ContaViewModel": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "numero": {
            "type": "integer",
            "format": "int32"
          },
          "titular": {
            "type": "string",
            "nullable": true
          },
          "saldo": {
            "type": "number",
            "format": "double"
          },
          "ativa": {
            "type": "boolean"
          },
          "tipo": {
            "enum": [
              "Corrente",
              "Poupanca"
            ],
            "type": "string"
          },
          "dataCriacao": {
            "type": "string",
            "format": "date-time"
          },
          "clienteId": {
            "type": "string",
            "format": "uuid"
          }
        },
        "additionalProperties": false
      },
      "EnderecoInputModel": {
        "required": [
          "bairro",
          "cep",
          "cidade",
          "estado",
          "logradouro",
          "numero"
        ],
        "type": "object",
        "properties": {
          "cep": {
            "minLength": 1,
            "type": "string"
          },
          "logradouro": {
            "minLength": 1,
            "type": "string"
          },
          "numero": {
            "minLength": 1,
            "type": "string"
          },
          "complemento": {
            "type": "string",
            "nullable": true
          },
          "bairro": {
            "minLength": 1,
            "type": "string"
          },
          "cidade": {
            "minLength": 1,
            "type": "string"
          },
          "estado": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "LoginInputModel": {
        "required": [
          "cpf",
          "senha"
        ],
        "type": "object",
        "properties": {
          "cpf": {
            "minLength": 1,
            "type": "string"
          },
          "senha": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ProblemDetails": {
        "type": "object",
        "properties": {
          "type": {
            "type": "string",
            "nullable": true
          },
          "title": {
            "type": "string",
            "nullable": true
          },
          "status": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "detail": {
            "type": "string",
            "nullable": true
          },
          "instance": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": { }
      },
      "TransacaoInputModel": {
        "required": [
          "valor"
        ],
        "type": "object",
        "properties": {
          "valor": {
            "maximum": 1000000,
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "TransacaoViewModel": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "tipo": {
            "type": "string",
            "nullable": true
          },
          "valor": {
            "type": "number",
            "format": "double"
          },
          "dataHora": {
            "type": "string",
            "format": "date-time"
          },
          "contaOrigemId": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          },
          "contaDestinoId": {
            "type": "string",
            "format": "uuid",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "TransferenciaInputModel": {
        "required": [
          "contaDestinoId",
          "valor"
        ],
        "type": "object",
        "properties": {
          "contaDestinoId": {
            "type": "string",
            "format": "uuid"
          },
          "valor": {
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "apiKey",
        "description": "Insira o token JWT aqui.\n\nExemplo: Bearer eyJhbGciOiJIUzI1NiIsInR5cCI...",
        "name": "Authorization",
        "in": "header"
      }
    }
  },
  "security": [
    {
      "Bearer": [ ]
    }
  ]
